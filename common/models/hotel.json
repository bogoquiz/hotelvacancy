{
  "name": "hotel",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "room_quantity": {
      "type": "number",
      "required": true,
      "default": 1
    }
  },
  "validations": [],
  "relations": {
    "cities": {
      "type": "belongsTo",
      "model": "city",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
